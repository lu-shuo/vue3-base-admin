		"dayjs": "^1.11.7",
		"echarts": "^5.4.2",
		"element-plus": "^2.3.3",
		"js-md5": "^0.7.3",
		"mitt": "^3.0.0",
		"nprogress": "^0.2.0",
		"pinia": "^2.0.34",
		"pinia-plugin-persistedstate": "^3.1.0",
		"sortablejs": "^1.15.0",
		"spark-md5": "^3.0.2",
		"vue": "^3.2.47",
		"vue-i18n": "9.3.0-beta.16",
		"vue-router": "4",
		"vue-simple-uploader": "^1.0.1"
