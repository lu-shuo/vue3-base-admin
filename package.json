		"sass": "^1.62.0",
		"standard-version": "^9.5.0",
		"stylelint": "^15.4.0",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-prettier": "^9.0.5",
		"stylelint-config-recess-order": "^4.0.0",
		"stylelint-config-recommended-scss": "^10.0.0",
		"stylelint-config-recommended-vue": "^1.4.0",
